{
  "name":"webforge\/configuration-tester",
  "description":"test your php configuration from cli or unit tests",
  "license":"MIT",
  "authors":[
    {
      "name":"Philipp Scheit",
      "email":"p.scheit@ps-webforge.com"
    }
  ],
  "minimum-stability":"dev",
  "require":{
    "guzzle/guzzle": "3.7.4@stable"
  },
  "autoload":{
    "psr-0":{
      "Webforge\\Setup\\ConfigurationTester":[
        "src\/php\/",
        "tests\/php\/"
      ]
    }
  },
  "extra":{
    "branch-alias":{
      "dev-master":"1.0.x-dev"
    }
  },
  "require-dev": {
    "webforge/testplate": "1.*"
  }
}
